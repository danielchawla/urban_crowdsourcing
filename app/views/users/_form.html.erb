<%= form_for user do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.label :age %>:
  <%= f.text_field :age %><br />

  <%= f.label :occupation %>:
  <%= f.text_field :occupation %><br />

  <%= f.label :gender %>:
  <%= f.text_field :gender %><br />

  <br/>
  <%= f.submit %>
<% end %>
 